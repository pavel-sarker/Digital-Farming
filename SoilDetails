        initComponents();
        conn=Javaconnect.ConnerDb();
        jTable1();
    }
    
    public void jTable1() {
        try {
            String sql = "SELECT * FROM soil";
            pst = conn.prepareStatement(sql);

            rs = pst.executeQuery();
            jTable1.setModel(DbUtils.resultSetToTableModel(rs));

        } catch (Exception e) {
            JOptionPane.showMessageDialog(null, e);
        }

    }

   
